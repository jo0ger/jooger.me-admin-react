	display:block;
	overflow:hidden
}
[data-view="markdown-body"] span.frame>span {
	display:block;
	float:left;
	width:auto;
	padding:7px;
	margin:13px 0 0;
	overflow:hidden;
	border:1px solid #dfe2e5
}
[data-view="markdown-body"] span.frame span img {
	display:block;
	float:left
}
[data-view="markdown-body"] span.frame span span {
	display:block;
	padding:5px 0 0;
	clear:both;
	color:#24292e
}
[data-view="markdown-body"] span.align-center {
	display:block;
	overflow:hidden;
	clear:both
}
[data-view="markdown-body"] span.align-center>span {
	display:block;
	margin:13px auto 0;
	overflow:hidden;
	text-align:center
}
[data-view="markdown-body"] span.align-center span img {
	margin:0 auto;
	text-align:center
}
[data-view="markdown-body"] span.align-right {
	display:block;
	overflow:hidden;
	clear:both
}
[data-view="markdown-body"] span.align-right>span {
	display:block;
	margin:13px 0 0;
	overflow:hidden;
	text-align:right
}
[data-view="markdown-body"] span.align-right span img {
	margin:0;
	text-align:right
}
[data-view="markdown-body"] span.float-left {
	display:block;
	float:left;
	margin-right:13px;
	overflow:hidden
}
[data-view="markdown-body"] span.float-left span {
	margin:13px 0 0
}
[data-view="markdown-body"] span.float-right {
	display:block;
	float:right;
	margin-left:13px;
	overflow:hidden
}
[data-view="markdown-body"] span.float-right>span {
	display:block;
	margin:13px auto 0;
	overflow:hidden;
	text-align:right
}
[data-view="markdown-body"] code,[data-view="markdown-body"] tt {
	padding:0;
	padding-top:0.2em;
	padding-bottom:0.2em;
	margin:0;
	font-size:85%;
	background-color:rgba(27,31,35,0.05);
	border-radius:3px
}
[data-view="markdown-body"] code::before,[data-view="markdown-body"] code::after,[data-view="markdown-body"] tt::before,[data-view="markdown-body"] tt::after {
	// letter-spacing:-0.2em;
	content:"\00a0"
}
[data-view="markdown-body"] code br,[data-view="markdown-body"] tt br {
	display:none
}
[data-view="markdown-body"] del code {
	text-decoration:inherit
}
[data-view="markdown-body"] pre {
	word-wrap:normal
}
[data-view="markdown-body"] pre>code {
	padding:0;
	margin:0;
	font-size:100%;
	word-break:normal;
	white-space:pre;
	background:transparent;
	border:0
}
[data-view="markdown-body"] .highlight {
	margin-bottom:16px
}
