	text-align:right;
	background:#fff;
	border:0
}
[data-view="markdown-body"] .csv-data tr {
	border-top:0
}
[data-view="markdown-body"] .csv-data th {
	font-weight:600;
	background:#f6f8fa;
	border-top:0
}

[data-view="markdown-body"] {
	ul, ol {
		list-style: inherit;
	}
}