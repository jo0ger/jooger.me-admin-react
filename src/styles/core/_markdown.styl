	width:100%;
	overflow:auto
}
[data-view="markdown-body"] table th {
	font-weight:600
}
[data-view="markdown-body"] table th,[data-view="markdown-body"] table td {
	padding:6px 13px;
	border:1px solid #dfe2e5
}
