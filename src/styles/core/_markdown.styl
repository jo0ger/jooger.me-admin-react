	background-color:#f6f8fa;
	border-radius:3px
}
[data-view="markdown-body"] pre code,[data-view="markdown-body"] pre tt {
	display:inline;
	max-width:auto;
	padding:0;
	margin:0;
	overflow:visible;
	line-height:inherit;
	word-wrap:normal;
	background-color:transparent;
	border:0
}
[data-view="markdown-body"] pre code::before,[data-view="markdown-body"] pre code::after,[data-view="markdown-body"] pre tt::before,[data-view="markdown-body"] pre tt::after {
	content:normal
}
[data-view="markdown-body"] .csv-data td,[data-view="markdown-body"] .csv-data th {
	padding:5px;
	overflow:hidden;
	font-size:12px;
	line-height:1;
	text-align:left;
	white-space:nowrap
}
